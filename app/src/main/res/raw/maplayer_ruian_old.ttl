@prefix xsd:  <http://www.w3.org/2001/XMLSchema#> .
@prefix void: <http://rdfs.org/ns/void#> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix sd: <http://www.w3.org/ns/sparql-service-description#> .
@prefix s: <http://schema.org/> .
@prefix ms: <http://matej.snoha.info/dp/> .
@prefix ruian: <http://ruian.linked.opendata.cz/ontology/> .
@prefix sh: <http://www.w3.org/ns/shacl#> .

<http://matej.snoha.info/dp/resource/mapLayer/ruian-old>

    a ms:MapLayer ;
    a void:Dataset ;

    dcterms:title "RUIAN (old)" ;
    dcterms:description "RUIAN - registry of land identification, addresses and properties of the Czech Republic" ;
    dcterms:publisher [ a s:Person ;
                        s:name "Matej Snoha" ;
                        s:email "matej@snoha.info" ] ;
    dcterms:modified "2017-10-11"^^xsd:date ;
    dcterms:license <http://www.opendatacommons.org/licenses/by/> ;

    ms:service [  a sd:Service ;
                  sd:endpoint "https://ldm.matej.snoha.info/fuseki/doubleshot/query" ;
                  sd:supportedLanguage sd:SPARQL11Query ;
                  ms:jenaSpatial true ;
               ];

    ms:mapLayerStructure [  a ms:MapLayerStructure ;
                          ms:addressPointType ruian:AdresniMisto ;
                          ms:addressPath ( s:name ) ; #TODO similar as data description
                          ms:latitudePath ( ruian:adresniBod s:geo s:latitude ) ;
                          ms:longitudePath ( ruian:adresniBod s:geo s:longitude) ] .
