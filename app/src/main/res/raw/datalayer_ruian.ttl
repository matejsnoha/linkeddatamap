@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xsd:  <http://www.w3.org/2001/XMLSchema#> .
@prefix void: <http://rdfs.org/ns/void#> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix sd: <http://www.w3.org/ns/sparql-service-description#> .
@prefix s: <http://schema.org/> .
@prefix ms: <http://matej.snoha.info/dp/> .
@prefix ruian: <https://ruian.linked.opendata.cz/slovník/> .
@prefix sh: <http://www.w3.org/ns/shacl#> .

<http://matej.snoha.info/dp/resource/dataLayer/ruian>

    a ms:DataLayer ;
    a s:Dataset ;

    dcterms:title "RUIAN" ;
    dcterms:description "RUIAN - registry of land identification, addresses and properties of the Czech Republic" ;
    dcterms:publisher [ a s:Person ;
                        s:name "Matej Snoha" ;
                        s:email "matej@snoha.info" ] ;
    dcterms:modified "2017-10-02"^^xsd:date ;
    dcterms:license <http://www.opendatacommons.org/licenses/by/> ;

    ms:service [  a sd:Service ;
                  sd:endpoint "https://ruian.linked.opendata.cz/sparql" ;
                  sd:supportedLanguage sd:SPARQL11Query ;
                  # sd:resultFormat <http://www.w3.org/ns/formats/RDF_XML>, <http://www.w3.org/ns/formats/Turtle> ;
               ];

    ms:dataLayerStructure [ a ms:DataLayerStructure ;
                          ms:dataPointType ruian:StavebníObjekt ;
                          ms:dataName s:name ;
                          ms:dataDescription ( "Pocet bytu " ruian:početBytů ", Pocet podlazi " ruian:početPodlaží ) ;
                          ms:mapPointPath [ sh:inversePath ruian:stavebníObjekt ] ] ;

    ms:mapLayer <http://matej.snoha.info/dp/resource/mapLayer/ruian> .
