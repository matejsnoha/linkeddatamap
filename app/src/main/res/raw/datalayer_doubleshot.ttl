@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xsd:  <http://www.w3.org/2001/XMLSchema#> .
@prefix void: <http://rdfs.org/ns/void#> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix sd: <http://www.w3.org/ns/sparql-service-description#> .
@prefix s: <http://schema.org/> .
@prefix ms: <http://matej.snoha.info/dp/> .
@prefix sh: <http://www.w3.org/ns/shacl#> .

<http://matej.snoha.info/dp/resource/dataLayer/ds>

    a ms:DataLayer ;
    a void:Dataset ;

    dcterms:title "DoubleShot" ;
    dcterms:description "Coffee shops serving DoubleShot coffee" ;
    dcterms:publisher [ a s:Person ;
                        s:name "Matej Snoha" ;
                        s:email "matej@snoha.info" ] ;
    dcterms:modified "2017-10-11"^^xsd:date ;
    dcterms:license <http://www.opendatacommons.org/licenses/by/> ;

    ms:dataLayerStructure [ a ms:DataLayerStructure ;
                          ms:dataPointType s:CafeOrCoffeeShop ;
                          ms:dataName s:name ;
                          ms:dataDescription ( "Rating " s:aggregateRating ) ;
                          ms:mapPointPath s:geo ] ;

    ms:mapLayer <http://matej.snoha.info/dp/resource/mapLayer/ds> .
