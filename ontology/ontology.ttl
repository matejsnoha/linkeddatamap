@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix void: <http://rdfs.org/ns/void#> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix sd: <http://www.w3.org/ns/sparql-service-description#> .
@prefix s: <http://schema.org/> .
@prefix ms: <http://matej.snoha.info/dp/> .

ms:
    a owl:Ontology ;
    dcterms:title "Linked Data Map Ontology"@en ;
    dcterms:description "Linked Data Map - visualizing Linked Data on a map"@en ;
    dcterms:publisher [ a s:Person ;
                        s:name "Matej Snoha"@en ;
                        s:email "matej@snoha.info" ] ;
    dcterms:modified "2017-09-20"^^xsd:date ;
    dcterms:license <http://www.opendatacommons.org/licenses/by/> .

#################################################################
#    Classes
#################################################################

###  http://matej.snoha.info/dp/Layer
ms:Layer
    a rdfs:Class ;
    rdfs:subClassOf void:DataSet ;
    rdfs:label "Layer"@en ;
    rdfs:comment "Base abstract class for Layers"@en .

###  http://matej.snoha.info/dp/MapLayer
ms:MapLayer
    a rdfs:Class ;
    rdfs:subClassOf ms:Layer ;
    rdfs:label "Map Layer"@en ;
    rdfs:comment "Map Layer with address points with address/lat/lng"@en .

###  http://matej.snoha.info/dp/DataLayer
ms:DataLayer
    a rdfs:Class ;
    rdfs:subClassOf ms:Layer ;
    rdfs:label "Data Layer"@en ;
    rdfs:comment "Data Layer with data points with name/description linked to a Map Layer"@en .

###  http://matej.snoha.info/dp/MapLayerStructure
ms:MapLayerStructure
    a rdfs:Class ;
    rdfs:subClassOf void:DataSet ;
    rdfs:label "Map Layer structure"@en ;
    rdfs:comment "Structure of the Map Layer: address point type, address path, latitude path, longitude path"@en .

###  http://matej.snoha.info/dp/DataLayerStructure
ms:DataLayerStructure
    a rdfs:Class ;
    rdfs:subClassOf void:DataSet ;
    rdfs:label "Data Layer structure"@en ;
    rdfs:comment "Structure of the Data Layer: data point type, data name, data description, linked map point path"@en .

#################################################################
#    Properties
#################################################################

###  http://matej.snoha.info/dp/service
ms:service
    a rdf:Property ;
    rdfs:domain ms:Layer ;
    rdfs:range sd:Service ;
    rdfs:label "SPARQL service description"@en ;
    rdfs:comment "Property for a SPARQL service description (endpoint, named graph, etc.)"@en .

###  http://matej.snoha.info/dp/jenaSpatial
ms:jenaSpatial
    a rdf:Property ;
    rdfs:domain sd:Service ;
    rdfs:range xsd:boolean ;
    rdfs:label "Jena Spatial support"@en ;
    rdfs:comment "Property (flag) that indicates if the SPARQL endpoint supports Jena Spatial"@en .

###  http://matej.snoha.info/dp/addressPointType
ms:addressPointType
    a rdf:Property ;
    rdfs:domain ms:MapLayerStructure ;
    rdfs:range rdfs:Class ;
    rdfs:label "Address Point type"@en ;
    rdfs:comment "Class of address points"@en .

###  http://matej.snoha.info/dp/addressPath
ms:addressPath
    a rdf:Property ;
    rdfs:domain ms:MapLayerStructure ;
    rdfs:range xsd:string ;
    rdfs:label "Address path"@en ;
    rdfs:comment "Property path to address points"@en .

###  http://matej.snoha.info/dp/latitudePath
ms:latitudePath
    a rdf:Property ;
    rdfs:domain ms:MapLayerStructure ;
    rdfs:range xsd:string ;
    rdfs:label "Latitude path"@en ;
    rdfs:comment "Property path to WGS84 latitude"@en .

###  http://matej.snoha.info/dp/longitudePath
ms:longitudePath
    a rdf:Property ;
    rdfs:domain ms:MapLayerStructure ;
    rdfs:range xsd:string ;
    rdfs:label "Longitude path"@en ;
    rdfs:comment "Property path to WGS84 longitude"@en .

###  http://matej.snoha.info/dp/dataPointType
ms:dataPointType
    a rdf:Property ;
    rdfs:domain ms:DataLayerStructure ;
    rdfs:range rdfs:Class ;
    rdfs:label "Data Point type"@en ;
    rdfs:comment "Class of data points"@en .

###  http://matej.snoha.info/dp/dataName
ms:dataName
    a rdf:Property ;
    rdfs:domain ms:DataLayerStructure ;
    rdfs:range xsd:string ;
    rdfs:label "Data name"@en ;
    rdfs:comment "Property path to data name"@en .

###  http://matej.snoha.info/dp/dataDescription
ms:dataDescription
    a rdf:Property ;
    rdfs:domain ms:DataLayerStructure ;
    rdfs:range rdf:List ;
    rdfs:label "Data description"@en ;
    rdfs:comment "List of literals and property paths to build a description"@en .

###  http://matej.snoha.info/dp/mapPointPath
ms:mapPointPath
    a rdf:Property ;
    rdfs:domain ms:DataLayerStructure ;
    rdfs:range xsd:string ;
    rdfs:label "Map Address Point path"@en ;
    rdfs:comment "Property path to linked map points"@en .

###  http://matej.snoha.info/dp/mapLayer
ms:mapLayer
    a rdf:Property ;
    rdfs:domain ms:DataLayer ;
    rdfs:range ms:MapLayer ;
    rdfs:label "Map Layer"@en ;
    rdfs:comment "Property for linking a map layer to a data layer"@en .

#################################################################
